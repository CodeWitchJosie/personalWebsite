{
  "name": "@jlcasto/personalWebsite",
  "description": "Personal website for Jocelyn Irwin",
  "repository": {
    "type": "git",
    "url": "https://github.com/jlcasto/personalWebsite"
  },
  "version": "1.0.0",
  "author": "Jocelyn Irwin <jlcasto@users.noreply.github.com>",
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop",
    "develop": "gatsby develop",
    "clean": "rm -rf public && rm -rf .cache",
    "clean-build": "rm -rf public && rm -rf .cache && gatsby build",
    "build": "gatsby build",
    "tsc": "tsc",
    "lint": "tslint -p .",
    "prettier": "prettier --write '**'",
    "gatsby": "gatsby",
    "serve": "gatsby serve",
    "test": "jest"
  },
  "dependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.6",
    "@types/react-helmet": "^5.0.8",
    "@types/react-scroll": "^1.5.4",
    "babel-preset-gatsby": "^0.1.4",
    "bootstrap": "^4.3.1",
    "gatsby": "^2.30.0",
    "gatsby-background-image": "^0.2.8-beta",
    "gatsby-image": "^2.0.35",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-google-gtag": "^2.6.0",
    "gatsby-plugin-manifest": "^2.0.26",
    "gatsby-plugin-netlify": "^2.0.13",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.11",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-sharp": "^2.0.32",
    "gatsby-plugin-typescript": "^2.0.11",
    "gatsby-source-filesystem": "^2.0.28",
    "gatsby-transformer-json": "^2.1.11",
    "gatsby-transformer-sharp": "^2.1.17",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-bootstrap": "^1.4.0",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-markdown": "^5.0.3",
    "react-scroll": "^1.7.11",
    "sass": "^1.17.3",
    "typescript": "^3.1.6"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.8.1",
    "@types/jest": "^23.3.9",
    "@types/react-test-renderer": "^16.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "prettier": "^1.16.4",
    "react-test-renderer": "^16.6.3",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "tslint-react": "^3.6.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*\\.([tj]sx?)|(\\.|/)(test|spec))\\.([tj]sx?)$",
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/jest/__mocks__/fileMock.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "setupFiles": [
      "<rootDir>/jest/loadershim.js"
    ]
  }
}
